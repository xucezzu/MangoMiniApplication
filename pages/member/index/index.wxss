.member-body {
  position: fixed;
  height: 100%;
  width: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  overflow: scroll;
}

.member-body-avatar-background {
  display: flex;
  justify-content: center;
  align-items: flex-end;
  width: 400rpx;
  height: 400rpx;
  background-repeat: no-repeat;
  margin-top: 80rpx;
}

.member-body-user-name{
  font-size: 25rpx;
  margin-bottom:20rpx;
  color: #636363
}

.member-body-content{
  display: flex;
  justify-content: center;
  width: 500rpx;
  height: 400rpx;
}
